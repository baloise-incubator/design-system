{
  "name": "@baloise/design-system-next-css",
  "version": "10.15.9",
  "description": "The Baloise Design-System is an open source project for styling awesome web applications that follow the Baloise corporate styling guidelines.",
  "repository": {
    "type": "git",
    "url": "https://github.com/baloise/design-system.git"
  },
  "bugs": {
    "url": "https://github.com/baloise/design-system/issues"
  },
  "homepage": "https://design.baloise.dev",
  "contributors": [
    "Gery Hirschfeld <gerhard.hirschfeld@baloise.ch> (https://github.com/hirsch88)",
    "Yannick Holzenkamp <yannick.holzenkamp@baloise.ch> (https://github.com/yannickholzenkamp)",
    "Petar Nobilo <petar.nobilo@baloise.ch> (https://github.com/nobilo)",
    "Laurent Tauber <laurent.tauber@baloise.ch> (https://github.com/Gagne87)",
    "Mladen Planinicic <mladen.planinicic@baloise.ch> (https://github.com/mladenplaninicic)",
    "Andreas Stebler <andreas.stebler@baloise.ch> (https://github.com/Clastzoo)"
  ],
  "keywords": [
    "design-system",
    "web-components",
    "stenciljs"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "npm run build:clean && npm run build:scss && npm run build:css",
    "serve": "npm run serve:fonts && npm run serve:http",
    "serve:fonts": "copyfiles --flat node_modules/@baloise/design-system-next-fonts/lib/* assets/fonts",
    "serve:http": "http-server --cors -c-1",
    "build:clean": "rimraf css && rimraf scss/generated",
    "build:scss": "node .build/generate.js",
    "build:css": "node .build/sass",
    "update:registry": "sed -i -e \"s#nexus.balgroupit.com/repository/npm#registry.npmjs.org#g\" package-lock.json\n"
  },
  "main": "scss/baloise-design-system.sass",
  "style": "css/baloise-design-system.min.css",
  "files": [
    "css/",
    "scss/"
  ],
  "dependencies": {
    "@baloise/design-system-next-fonts": "10.15.9",
    "@baloise/design-system-next-tokens": "10.15.9"
  },
  "devDependencies": {
    "bulma": "~0.9.3",
    "copyfiles": "~2.4.1",
    "minireset.css": "~0.0.7",
    "normalize.css": "~8.0.1",
    "sass": "~1.54.0",
    "ts-node": "~10.9.1"
  },
  "gitHead": "2bbab92d8cb613be35deecb8d261ec1dc7e398fd"
}
